{% include 'partials/header-dashboard' %}

<section>
  <div class="container">
    <div class="feed">
      <div class="row th">
        <div class="two columns" >
          <a href="">Interaction <i class="icon-arrow-down"></i></a>
        </div>
        <div class="four columns" >
          <a href="">Email</a>
        </div>
        <div class="two columns center" >
          <a href="">Deliveries (Bounced)</a>
        </div>
        <div class="two columns center" >
          <a href="">Opens (Unique)</a>
        </div>
        <div class="two columns center" >
          <a href="">Clicks (Unique)</a>
        </div>
      </div>
      {% for event in data %}
      <div class="row">
        <div class="two columns" title="{{ event.last_seen | date: "%a, %b %d, %y" }}">
          {{ event.timeago }}
        </div>
        <div class="four columns">
          <a href="/users/{{ event.email }}">{{ event.email }}</a>
        </div>
        <div class="two columns center" >
          <a href="/users/{{ event.email }}">{{ event.delivered }} <span class="fade">(0)</span></a>
        </div>
        <div class="two columns center" >
          <a href="/users/{{ event.email }}">{{ event.opened }} <span class="fade">({{ event.unique_opens }})</span></a>
        </div>
        <div class="two columns center" >
          <a href="/users/{{ event.email }}">{{ event.clicked }} <span class="fade">({{ event.unique_clicks }})</span></a>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</section>

</body>
</html>
